<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
			  xmlns:tools="http://schemas.android.com/tools"
			  android:layout_width="match_parent"
			  android:layout_height="match_parent"
			  android:orientation="vertical"
			  android:paddingBottom="@dimen/activity_vertical_margin"
			  android:paddingLeft="@dimen/activity_horizontal_margin"
			  android:paddingRight="@dimen/activity_horizontal_margin"
			  android:paddingTop="@dimen/activity_vertical_margin"
			  tools:activity=".MainActivity">

	<Button
		android:id="@+id/buttonRxJavaRequest"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="RxJava Call"/>

	<Button
		android:id="@+id/buttonBlockingRequest"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="Blocking Call"/>

	<Button
		android:id="@+id/buttonAsyncRequest"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="Async Request"/>

	<Button
		android:id="@+id/buttonInvalidateToken"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="Invalidate Token"/>

	<Button
		android:id="@+id/buttonResetPrefAccount"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="Reset active Account"/>

	<Button
		android:id="@+id/buttonAddAccount"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:text="Add a new Account"/>

</LinearLayout>
